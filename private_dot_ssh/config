Host *
  IdentityAgent "~/.1password/agent.sock"

Host aws-production
  ProxyCommand ~/.ssh/aws-ssm-ec2-proxy-command.sh %r %p ~/.ssh/id_ed25519.pub
  StrictHostKeyChecking no
  User ec2-user
  ServerAliveInterval=120
  RequestTTY no

Host aws-acceptance
  ProxyCommand ~/.ssh/aws-ssm-ec2-proxy-command.sh %r %p ~/.ssh/id_ed25519.pub
  StrictHostKeyChecking no
  User ec2-user
  ServerAliveInterval=120
  RequestTTY no

Host aws-test
  ProxyCommand ~/.ssh/aws-ssm-ec2-proxy-command.sh %r %p ~/.ssh/id_ed25519.pub
  StrictHostKeyChecking no
  User ec2-user
  ServerAliveInterval=120
  RequestTTY no
